{
  "name": "todo-ts",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^15.6.1",
    "react-addons-linked-state-mixin": "^15.6.0",
    "react-dom": "^15.6.1",
    "react-helmet": "^5.1.3",
    "react-mixin": "^3.1.0",
    "react-scripts-ts": "2.6.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css}": [
      "prettier --single-quote --semi false --write",
      "git add"
    ]
  },
  "scripts": {
    "prettier": "prettier --single-quote --semi false --write \"src/**/*.{js,jsx,ts,tsx,json,css}\"",
    "precommit": "lint-staged",
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "storybook": "start-storybook -p 9001"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.0",
    "@storybook/react": "^3.2.4",
    "@types/jest": "^20.0.7",
    "@types/node": "^8.0.22",
    "@types/react": "^16.0.2",
    "@types/react-addons-linked-state-mixin": "^0.14.18",
    "@types/react-dom": "^15.5.2",
    "@types/react-helmet": "^5.0.3",
    "@types/react-mixin": "^2.0.30",
    "@types/storybook__addon-actions": "^3.0.1",
    "@types/storybook__react": "^3.0.4",
    "husky": "^0.14.3",
    "lint-staged": "^4.0.3",
    "prettier": "^1.5.3"
  }
}
